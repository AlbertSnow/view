<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/root_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.albert.constraintapp.MainActivity">

    <Button
        android:id="@+id/cmd_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CommandClick"
        />

    <Button
        android:id="@+id/toast_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ToastClick"
        />

    <Button
        android:id="@+id/inner_service_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="InnerService"
        />

    <Button
        android:id="@+id/out_service_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="OutService"
        />
</LinearLayout>
